tags: $:/tags/Global
title: $:/lithic/macros/embed-stream
type: text/vnd.tiddlywiki

\procedure .stream-recurse()
<ul>
  <$list filter="[enlist{!!stream-list}]">
    <li>
      <$transclude mode="block"/>
      <<.stream-recurse>>
    </li>
  </$list>
</ul>
\end

\procedure embed-stream(parent)
<$tiddler tiddler=<<parent>>>
  <<.stream-recurse>>
</$tiddler>
\end
